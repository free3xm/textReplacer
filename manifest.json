{
 "manifest_version": 2,
 "name": "Text Replacer",
 "version": "1.0.1",
 "default_locate": "en",
 "permissions": ["tabs", "activeTab"],
 "content_scripts":[
   {
     "matches": ["<all_urls>"],
     "all_frames": true,
     "run_at":"document_end",
     "js":["script.js"]
   }
 ]

}
